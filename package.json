{
  "name": "js_work",
  "version": "1.0.0",
  "description": "Test backend project",
  "main": "./src/app.ts",
  "scripts": {
    "test": "npm run rollback && npm run migrate && ts-node node_modules/jasmine/bin/jasmine --random=false --config=./spec/support/jasmine.json",
    "coverage": "nyc -r text -e .ts -x \"tests/*.test.ts\" npm run test",
    "start": "npm run rollback && npm run migrate && node ./dist/app.js",
    "build-dist": "./node_modules/typescript/bin/tsc",
    "rollback": "knex --knexfile src/knexfile.ts migrate:rollback",
    "migrate": "knex --knexfile src/knexfile.ts migrate:latest"
  },
  "keywords": [],
  "author": "Martin Kozak",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^18.0.0",
    "ajv": "^8.11.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.1.0",
    "koa": "^2.13.4",
    "koa-bearer-token": "^2.0.2",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-router": "^10.1.1",
    "nodemon": "^2.0.16",
    "objection": "^3.0.1",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/knex": "^0.16.1",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-compose": "^3.2.5",
    "@types/koa-router": "^7.4.4",
    "jasmine": "^4.2.1",
    "nyc": "^15.1.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
